    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug LCD device",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/lcd_display.out",
            "args": [],
            "preLaunchTask": "Build LCD project",
            "miDebuggerPath": "${workspaceRoot}/.vscode/gdb_root",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
        },
    ],
}